window/size/viewport_height=1280
window/size/borderless=true
window/size/window_width_override=645
window/size/window_height_override=1398
window/stretch/mode="viewport"
window/stretch/aspect="expand"
window/handheld/orientation=1

[editor]

movie_writer/movie_file="/Users/iohann/Movies/boba_fusion_recording.avi"

[input_devices]

pointing/emulate_touch_from_mouse=true

[internationalization]

locale/translations=PackedStringArray("res://menu/translation/translation.en.translation", "res://menu/translation/translation.es.translation", "res://menu/translation/translation.fr.translation")

[rendering]

